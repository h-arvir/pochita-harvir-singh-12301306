{
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "regions": ["iad1"],
  "env": {
    "GEMINI_API_KEY": "@gemini_api_key"
  },
  "functions": {
    "api/index.py": {
      "runtime": "python3.13"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "api/index.py"
    }
  ]
}
